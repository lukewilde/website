header
  margin 19px 0

.header-title

  img
    float left

  h1
    float left
    padding-left 22px

.blog-title
  margin-top 35px

  span
    display block
    font-size 11.1px

.navbar-nav
  float right
  margin 0
  padding-top 7px

  li
    float left
    margin-top 28px

h1.blog-title, .navbar-nav li

  a
    color gray

  a:hover
    text-decoration none
    background-color transparent
    color darkgray

.lead

  padding 25px 0
  margin-bottom 0

  background-color #f5f5f5
  text-align center

.lead-bottom
  margin-bottom 20px
