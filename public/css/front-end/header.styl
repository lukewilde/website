header
  margin 19px 0

.header-title

  img
    float left
    box-shadow 0 10px 23px -18px rgba(0, 0, 0, 0.44)

  h1
    float left
    padding-left 22px

.blog-title
  margin-top 35px

  span
    display block
    font-size 11.1px

.navbar-nav
  float right
  margin 0
  padding-top 7px

  li
    float left
    margin-top 28px

h1.blog-title, .navbar-nav li

  a
    color gray

  a:hover
    text-decoration none
    background-color transparent
    color darkgray

.title-container {
  margin -53px 0 10px 0
}

.lead

  padding 25px 0
  margin-bottom 0
  background-color #f5f5f5
  text-align center

.lead-svg
  margin-bottom 30px
